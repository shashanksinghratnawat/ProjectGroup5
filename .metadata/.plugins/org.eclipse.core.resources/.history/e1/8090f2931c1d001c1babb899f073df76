
public class ArmStrong {
	public static void main(String[] args) {
			int n;
			int num=n;
			double sum=0;
			while(n!=0) {

			int r=n%10;

			sum=sum+Math.pow((double)r, (double) 3); 
			n=n/10;
			}
			int a=(int) sum;

			if(a==num)
			System.out.println("arm strong "+a);
			else
			System.out.println("not armstron g"+a);

			}  
	}
}
