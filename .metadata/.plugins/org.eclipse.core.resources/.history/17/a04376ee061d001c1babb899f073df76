public class Main {
public static void main(String[] args) {
System.out.println("Hello from kunal");
int fact = 1 ,result = 0 , rem , ptr,i ,num = 145;

ptr = num;
while(num>0){
rem = num%10;
for(i = 1 ; i<=rem ; i++){
fact = fact*i;
}
result = result + fact;
fact = 1;
num = num / 10;
}
if(ptr == result){
System.out.println(result + " "+"is a strong number" );
}
else{
System.out.println(result + " is not a strong number");
}




}
}
